{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Codegen",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/rawl-codegen/main.py",
            "console": "integratedTerminal",
            "args": "--input  ${workspaceFolder}/is-04/APIs/NodeAPI.raml",
            "cwd": "${workspaceFolder}/out/build/Debugging"
        },
        {
            "name": "(gdb) iouring-echo",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/build/Debugging/io_uring_interface",
            "args": ["--no-tune", "--ping", "142.250.184.238"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) unittests",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/build/Debugging/tests/unittests",
            "args": [
                "--gtest_filter=*TestSendPacket*"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}